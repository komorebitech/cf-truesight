{
  "name": "@cityflo/truesight-web-sdk",
  "version": "0.1.2",
  "description": "TrueSight Web Analytics SDK",
  "type": "module",
  "main": "dist/truesight.cjs.js",
  "module": "dist/truesight.esm.js",
  "browser": "dist/truesight.umd.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/truesight.esm.js",
      "require": "./dist/truesight.cjs.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "rollup -c",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "analytics",
    "truesight",
    "event-tracking",
    "cityflo"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cityflo/cf-true-sight.git",
    "directory": "sdks/web"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "fzstd": "^0.1.1"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "fake-indexeddb": "^6.0.0",
    "rollup": "^4.0.0",
    "@rollup/plugin-typescript": "^12.0.0",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-commonjs": "^28.0.0",
    "@rollup/plugin-terser": "^0.4.0",
    "tslib": "^2.6.0",
    "typescript": "^5.6.0",
    "vitest": "^2.0.0"
  }
}
